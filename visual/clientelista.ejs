<!DOCTYPE html>
<html lang="pt-br">
<%- include('../visual/head.html'); %>
<body>

  <header>
    <%- include('../visual/menu.html'); %>
  </header>

  <main>
<div class="lista-usuarios">
    <h1>Usuários cadastrados</h1>
    <hr>
    <table border="1" cellpadding="0" cellspacing="0">
      <tr>
          <th>Nome</th>
          <th>CPF</th>
          <th>Endereço</th>
          <th>Email</th>
          <th>Senha</th>
          <th>Celular</th>
      </tr>
      <%
      Cliente.forEach((c, index) => {
      %>
      <tr>
          <td><%= c.nome %></td>
          <td><%= c.cpf %></td>
          <td><%= c.endereco %></td>
          <td><%= c.email %></td>
          <td><%= c.senha %></td>
          <td><%= c.celular %></td>
      </tr>
      <%
      })
      %>
  </table>
</div>
  </main>

    <%- include('../visual/footer.html'); %>
</body>
<script src="https://kit.fontawesome.com/1165876da6.js" crossorigin="anonymous"></script>

</html>